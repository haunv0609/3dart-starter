    "description": "packages starter in all project of 3DART",
    "license": "Unlicense",
    "autoload": {
        "psr-4": {
            "Haunv\\artStarter\\": "src/"
        },

        "files": [
            "src/Helpers/ArtHelper.php"
        ]
    },
    "authors": [
        {
            "name": "Nguyễn Văn Hậu",
